{"ast":null,"code":"import ToDoListServices from \"../services/ToDoListServices\";\nexport default {\n  name: \"to-do-list\",\n\n  data() {\n    return {\n      events: [],\n      currentEvent: null,\n      currentIndex: -1,\n      title: \"\"\n    };\n  },\n\n  methods: {\n    fetchAllEvents() {\n      ToDoListServices.getAll().then(response => {\n        this.events = response.data;\n        console.log(response.data);\n      }).catch(e => {\n        console.log(e);\n      });\n    },\n\n    refreshToDoList() {\n      this.fetchAllEvents();\n      this.currentEvent = null;\n      this.currentIndex = -1;\n    },\n\n    setActiveEvent(event, index) {\n      this.currentEvent = event;\n      this.currentIndex = event ? index : -1;\n    },\n\n    removeAllEvents() {\n      ToDoListServices.deleteAll().then(response => {\n        console.log(response.data);\n        this.refreshToDoList();\n      }).catch(e => {\n        console.log(e);\n      });\n    }\n\n  },\n\n  mounted() {\n    this.fetchAllEvents();\n  }\n\n};\n$(document).ready(function () {\n  $(\"#list-group-item\").on({\n    mouseenter: function () {\n      $(this).css({\n        \"text-decoration\": \"overline\"\n      });\n    }\n  });\n});","map":{"version":3,"mappings":"AAyCA,OAAOA,gBAAP,MAA6B,8BAA7B;AACA,eAAe;EACXC,IAAI,EAAE,YADK;;EAEXC,IAAI,GAAG;IACH,OAAO;MACPC,MAAM,EAAE,EADD;MAEPC,YAAY,EAAE,IAFP;MAGPC,YAAY,EAAE,CAAC,CAHR;MAIPC,KAAK,EAAE;IAJA,CAAP;EAMH,CATU;;EAUXC,OAAO,EAAE;IACLC,cAAc,GAAG;MACbR,gBAAgB,CAACS,MAAjB,GACCC,IADD,CACMC,QAAO,IAAK;QACd,KAAKR,MAAL,GAAcQ,QAAQ,CAACT,IAAvB;QACAU,OAAO,CAACC,GAAR,CAAYF,QAAQ,CAACT,IAArB;MACH,CAJD,EAKCY,KALD,CAKOC,KAAK;QACRH,OAAO,CAACC,GAAR,CAAYE,CAAZ;MACH,CAPD;IAQH,CAVI;;IAYLC,eAAe,GAAG;MACd,KAAKR,cAAL;MACA,KAAKJ,YAAL,GAAoB,IAApB;MACA,KAAKC,YAAL,GAAoB,CAAC,CAArB;IACH,CAhBI;;IAkBLY,cAAc,CAACC,KAAD,EAAQC,KAAR,EAAe;MACzB,KAAKf,YAAL,GAAoBc,KAApB;MACA,KAAKb,YAAL,GAAoBa,KAAI,GAAIC,KAAJ,GAAY,CAAC,CAArC;IACH,CArBI;;IAuBLC,eAAe,GAAG;MACdpB,gBAAgB,CAACqB,SAAjB,GACCX,IADD,CACM