/div>\r\n      <div v-else>\r\n        <br />\r\n        <p>Please click on an Event...</p>\r\n      </div>\r\n    </div>\r\n\r\n</div>\r\n\r\n</template>\r\n<script>\r\nimport ToDoListServices from \"../services/ToDoListServices\";\r\nexport default {\r\n    name: \"to-do-list\",\r\n    data() {\r\n        return {\r\n        events: [],\r\n        currentEvent: null,\r\n        currentIndex: -1,\r\n        title: \"\"\r\n        };\r\n    },\r\n    methods: {\r\n        fetchAllEvents() {\r\n            ToDoListServices.getAll()\r\n            .then(response => {\r\n                this.events = response.data;\r\n                console.log(response.data);\r\n            })\r\n            .catch(e => {\r\n                console.log(e);\r\n            });\r\n        },\r\n        \r\n        refreshToDoList() {\r\n            this.fetchAllEvents();\r\n            this.currentEvent = null;\r\n            this.currentIndex = -1;\r\n        },\r\n        \r\n        setActiveEvent(event, index) {\r\n            this.currentEvent = event;\r\n            this.currentIndex = event ? index : -1;\r\n        },\r\n        \r\n        removeAllEvents() {\r\n            ToDoListServices.deleteAll()\r\n            .then(response => {\r\n                console.log(response.data);\r\n                this.refreshToDoList();\r\n            })\r\n            .catch(e => {\r\n                console.log(e);\r\n            });\r\n        }\r\n\r\n    },\r\n    \r\n    mounted() {\r\n        this.fetchAllEvents();\r\n    }\r\n\r\n};\r\n\r\n$(document).ready(function(){\r\n    $(\"#list-group-item\").on({\r\n        mouseenter: function(){\r\n        $(this).css({\"text-decoration\": \"overline\"});\r\n        }\r\n    });    \r\n});\r\n</script>\r\n<style>\r\n.line-through {\r\n    text-decoration: line-through;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module"}